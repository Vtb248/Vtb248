<!DOCTYPE html>
<html>
<script src="plotly.js"></script>

<body>
<div id="myPlot" style="width:100%;max-width:700px"></div>

<script>
var exp = "x";

// Generate values
var xValues = [];
var yValues = [];
for (var x = 0; x <= 50; x += 1) {
  xValues.push(x);
  yValues.push(eval(exp));
}

// Define Data
var data = [{
  x: xValues,
  y: yValues,
  mode:"lines"
}];

// Define Layout
var layout = {title: "y = " + exp};

// Display using Plotly
Plotly.newPlot("myPlot", data, layout);
</script>

</body>
</html>
